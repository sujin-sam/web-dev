<%- include('partials/header') -%>

    <h1>Home</h1>
    <p>
        <%= content %>
    </p>
    <% postsList.forEach(post=> { %>
        <h1>
            <%= post.title %>
        </h1>
        <p>
            <%= (post.msg).substring(0, 100) + ' ...' %>
                <a href='/posts/<%= post.title %>'>Read More</a>
        </p>

        <% }); %>
            <%- include('partials/footer') -%>